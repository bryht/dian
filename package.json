{
  "name": "dian",
  "version": "0.7.3",
  "private": true,
  "main": "main.ts",
  "description": "a dictionary app",
  "homepage": ".",
  "author": {
    "name": "bryht",
    "email": "bryht@live.com"
  },
  "build": {
    "appId": "Dian",
    "productName": "Dian",
    "artifactName": "${productName}_Setup_${os}_${arch}.${ext}",
    "files": [
      "build/**/*",
      "main.ts"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "bryht",
        "repo": "dian",
        "releaseType": "release"
      }
    ],
    "extends": null,
    "directories": {
      "buildResources": "assets",
      "output": "release"
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icon.icns"
    }
  },
  "dependencies": {
    "@bryht/auto-complete-input": "^0.0.17",
    "@cliqz/adblocker-electron": "^1.20.3",
    "@popperjs/core": "^2.9.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/cheerio": "^0.22.24",
    "@types/electron-json-storage": "^4.5.0",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.23",
    "@types/mousetrap": "^1.6.5",
    "@types/pdfkit": "^0.11.1",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@types/react-redux": "^7.1.16",
    "@types/react-transition-group": "^4.4.1",
    "bootstrap": "^5.0.0-beta2",
    "cheerio": "^1.0.0-rc.5",
    "electron-is-dev": "^2.0.0",
    "electron-json-storage": "^4.4.0",
    "electron-log": "^4.3.2",
    "electron-updater": "^4.3.8",
    "fs-extra": "^10.0.0",
    "google-tts-api": "^2.0.1",
    "guesslanguage": "^0.2.0",
    "jquery": "^3.6.0",
    "mousetrap": "^1.6.5",
    "node-fetch": "^2.6.1",
    "node-google-translate-skidz": "^1.1.2",
    "node-powershell": "^4.0.0",
    "node-sass": "^6.0.1",
    "pdfkit": "^0.12.1",
    "popper.js": "^1.16.1",
    "react": "^17.0.1",
    "react-bootstrap-typeahead": "^5.1.4",
    "react-dom": "^17.0.1",
    "react-modal": "^3.12.1",
    "react-redux": "^7.2.2",
    "react-scripts": "^4.0.3",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@types/react-bootstrap-typeahead": "^5.1.3",
    "@types/react-modal": "^3.12.0",
    "@types/redux-logger": "^3.0.8",
    "concurrently": "^6.2.0",
    "electron": "^13.1.5",
    "electron-builder": "^22.10.5",
    "electron-reload": "^1.5.0",
    "redux-logger": "^3.0.6",
    "typescript": "^4.3.5",
    "wait-on": "^6.0.0"
  },
  "scripts": {
    "start": "concurrently \"react-scripts start\" \"wait-on http://localhost:3000  && electron .\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "package": "electron-builder -wm --publish=always",
    "package-local": "electron-builder -w"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "@typescript-eslint/no-redeclare": [
            0
          ]
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
